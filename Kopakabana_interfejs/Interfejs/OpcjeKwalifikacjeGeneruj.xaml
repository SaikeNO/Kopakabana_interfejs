<Window x:Class="Kopakabana.OpcjeKwalifikacjeGeneruj"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kopakabana"
        mc:Ignorable="d"
       Title="OpcjeKwalifikacjeGeneruj" Height="368" Width="1000">
    <Grid>
        <Button x:Name="WybierzSedziego" Content="Wybierz Sędziego" HorizontalAlignment="Left" Margin="30,42,0,0" VerticalAlignment="Top" FontSize="22" Click="WybierzSedziego_Click" Padding="10 0" />
        <ListBox x:Name="Rozgrywki" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="250,41,402,66"/>
        <ListBox x:Name="Tabela" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="603,41,25,66"/>
        <TextBlock HorizontalAlignment="Left" Margin="388,20,0,0" TextWrapping="Wrap" Text="Rozgrywki" VerticalAlignment="Top"/>
        <Button x:Name="Rozegraj" Content="Rozegraj" HorizontalAlignment="Left" Margin="30,78,0,0" VerticalAlignment="Top" FontSize="22" Click="Rozegraj_Click" Padding="10 0" Width="195"/>
        <TextBlock HorizontalAlignment="Left" Margin="772,20,0,0" TextWrapping="Wrap" Text="Tabela" VerticalAlignment="Top"/>
    </Grid>
</Window>
